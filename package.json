{
  "name": "mutix",
  "version": "0.0.1",
  "description": "A lightweight modern state management library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Protagonistss/mutix.git"
  },
  "keywords": [
    "state-management",
    "typescript",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Protagonistss/mutix/issues"
  },
  "homepage": "https://github.com/Protagonistss/mutix#readme",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
